
# Discord Configuration
DISCORD_BOT_TOKEN=your_bot_token_here
DISCORD_APPLICATION_ID=123456789012345678
# Guild settings are now stored dynamically in the database

# Discord bot configuration
BOT_TOKEN=
CLIENT_ID=
GLOBAL_COMMANDS=false
WEBHOOK_PORT=5001

# DAO NFT Contract (for free access verification)
DAO_NFT_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
DAO_NFT_FREE_MINTS_PER_TOKEN=3
DAO_NFT_VERIFICATION_INTERVAL_HOURS=24

# Blockchain Configuration
PRIMARY_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
SECONDARY_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your_alchemy_key
TERTIARY_RPC_URL=https://rpc.ankr.com/eth
WEBSOCKET_RPC_URL=wss://mainnet.infura.io/ws/v3/your_infura_key
CHAIN_ID=1

# Performance Settings
MAX_CONCURRENT_MINTS=75
MAX_QUEUE_SIZE=2000
QUEUE_PROCESSING_INTERVAL_MS=50
CONNECTION_POOL_SIZE=100
REQUEST_TIMEOUT_MS=8000
MINT_MAX_RETRIES=2 # number of times to retry a failed mint
GAS_MULTIPLIER=1.2 # increase to speed up replacement transactions (mint bot uses this)

# Predictive Failure Prevention
ENABLE_BALANCE_PRECHECK=true
MIN_BALANCE_BUFFER_ETH=0.15
ENABLE_GAS_ESTIMATION=true
MAX_GAS_PRICE_GWEI=200
REMOVE_FAILED_USERS_IMMEDIATELY=true

# Rate Limiting & Access Control
PAID_USER_DAILY_LIMIT=50
NFT_HOLDER_DAILY_LIMIT=10
COOLDOWN_BETWEEN_MINTS_SECONDS=30
MAX_MINTS_PER_PROJECT_PER_USER=5

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/mintbot_db
REDIS_URL=redis://localhost:6379
DATABASE_POOL_SIZE=20

# Security & Sessions
JWT_SECRET=your_super_secure_jwt_secret_here
SESSION_EXPIRE_HOURS=48
WALLET_CONNECTION_EXPIRE_MINUTES=15
SIGNATURE_MESSAGE="Authorize mint bot access"

# Monitoring & Notifications
LOG_LEVEL=info
DISCORD_LOG_CHANNEL_ID=123456789012345678
ALERT_CHANNEL_ID=987654321098765432
ENABLE_PERFORMANCE_MONITORING=true
ALERT_ON_HIGH_FAILURE_RATE=true
FAILURE_RATE_THRESHOLD=0.15
METRICS_PORT=9090
MINT_BOT_METRICS_PORT=9101
DETECTION_SCORE_THRESHOLD=1 # emit detection opportunities when score >= value

# Payment Integration (if using on-chain payments)
PAYMENT_TOKEN_ADDRESS=0x...
# Comma separated list of wallet addresses to receive payments
PAYMENT_RECEIVER_ADDRESSES=0x111...,0x222...,0x333...



# Web3 configuration
# At least PRIMARY_RPC_URL must be a WebSocket endpoint for the Rust mint bot
PRIMARY_RPC_URL=wss://eth.llamarpc.com
SECONDARY_RPC_URL=
TERTIARY_RPC_URL=
# Legacy variable used by older scripts
RPC_URL=
PRIVATE_KEY=
CONTRACT_ADDRESS=
MINT_GAS_LIMIT= # optional custom gas limit for NFT mints
ROUTER_ADDRESS=
WETH_ADDRESS=
USE_FLASHBOTS=false # enable to submit transactions via Flashbots
# Gas multiplier applied to EIP-1559 fees
# GAS_MULTIPLIER also applies here if set above

# Twitter OAuth
TWITTER_CLIENT_ID=
TWITTER_CLIENT_SECRET=
TWITTER_REDIRECT_URI=

